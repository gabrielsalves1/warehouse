<% if user_signed_in? %>
    <%= link_to 'Sair' %>
    Olá, <%= current_user.email %>
<% else %>
    <%= link_to 'Entrar', new_user_session_path %>
<% end %>

<h1>WareHouse App</h1>
<h3>Boas vindas ao sistema de gestão de estoques</h3>

<h2>Galpões cadastrados</h2>
<% @warehouses.each do |w|%>
    <%= link_to(w.name, warehouse_path(w.id)) %>
    <%= w.code %>
<% end %>

